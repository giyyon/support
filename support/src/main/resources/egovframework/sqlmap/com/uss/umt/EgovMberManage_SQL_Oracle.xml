    <update id="mberManageDAO.updateMber_S">
        <![CDATA[
            UPDATE COMTNGNRLMBER 
            SET MBER_ID          = #mberId#,
                MBER_NM          = #mberNm#,
                PASSWORD_HINT    = #passwordHint#,
                PASSWORD_CNSR    = #passwordCnsr#,
                IHIDNUM          = #ihidnum#,
                SEXDSTN_CODE     = #sexdstnCode#,
                ZIP              = #zip#,
                ADRES            = #adres#,
                AREA_NO          = #areaNo#,
